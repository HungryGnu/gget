{
    "test_blast_nt": {
        "type": "assert_equal",
        "args": {
            "sequence": "ATACTCAGTCACACAAGCCATAGCAGGAAACAGCGAGCTTGCAGCCTCAC",
            "limit": 1
        },
        "expected_result": [
            [
                "PREDICTED: Pan troglodytes CASP8 and FADD like apoptosis regulator (CFLAR), transcript variant X16, mRNA",
                "Pan troglodytes",
                "chimpanzee",
                9598,
                93.5,
                93.5,
                "100%",
                1e-15,
                "100.00%",
                1985,
                "XM_054679210.2"
            ]
        ]
    },
    "test_blast_nt_json": {
        "type": "assert_equal",
        "args": {
            "sequence": "ATACTCAGTCACACAAGCCATAGCAGGAAACAGCGAGCTTGCAGCCTCAC",
            "limit": 1,
            "json": true
        },
        "expected_result": [
            {
                "Description": "PREDICTED: Pan troglodytes CASP8 and FADD like apoptosis regulator (CFLAR), transcript variant X16, mRNA",
                "Scientific Name": "Pan troglodytes",
                "Common Name": "chimpanzee",
                "Taxid": 9598,
                "Max Score": 93.5,
                "Total Score": 93.5,
                "Query Cover": "100%",
                "E value": 0.0,
                "Per. Ident": "100.00%",
                "Acc. Len": 1985,
                "Accession": "XM_054679210.2"
            }
        ]
    },
    "test_blast_bad_seq": {
        "type": "error",
        "args": {
            "sequence": "BANANA123"
        },
        "expected_result": "ValueError"
    },
    "test_blast_bad_fasta": {
        "type": "error",
        "args": {
            "sequence": "banana.fa"
        },
        "expected_result": "FileNotFoundError"
    },
    "test_blast_bad_program": {
        "type": "error",
        "args": {
            "sequence": "tests/fixtures/muscle_aa_test.txt",
            "limit": 3,
            "program": "banana"
        },
        "expected_result": "ValueError"
    },
    "test_blast_db_missing": {
        "type": "error",
        "args": {
            "sequence": "tests/fixtures/muscle_aa_test.txt",
            "limit": 3,
            "program": "blastn"
        },
        "expected_result": "ValueError"
    },
    "test_blast_bad_db1": {
        "type": "error",
        "args": {
            "sequence": "tests/fixtures/muscle_aa_test.txt",
            "limit": 3,
            "program": "blastp",
            "database": "banana"
        },
        "expected_result": "ValueError"
    },
    "test_blast_bad_db2": {
        "type": "error",
        "args": {
            "sequence": "tests/fixtures/muscle_aa_test.txt",
            "limit": 3,
            "database": "banana"
        },
        "expected_result": "ValueError"
    }
}
